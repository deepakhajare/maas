include "{{named_rndc_conf_path}}";

# Zone declarations.
{{for zone in forward_zones}}
zone "{{zone.zone_name}}" {
    type master;
    file "{{zone.target_path}}";
};
{{endfor}}

# Reverse zone declarations.
{{for zone in reverse_zones}}
zone "{{zone.reverse_zone_name}}" {
    type master;
    file "{{zone.target_path}}";
};
{{endfor}}
